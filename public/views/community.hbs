  <body>
    {{>nav}}
    <div class="container custom-carousel">
        <div id="memory-care-carousel" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active custom-carousel-item">
                    <div class="custom-carousel-content bg-red">
                        <div class="row">
                            <div class="col-md">
                                <img src="/images/community/wentworth/carousel-1/cake.jpg" class="community-carousel-image">
                            </div>
                            <div class="col-md about-p-col">
                                <h3>Carrot Cake</h3>
                                <hr>
                                <p class="paragraph-font-family">
                                    
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="carousel-item custom-carousel-item">
                    <div class="custom-carousel-content bg-red">
                        <div class="row">
                            <div class="col-md">
                                <img src="/images/community/wentworth/carousel-1/ipad.jpg" class="community-carousel-image">
